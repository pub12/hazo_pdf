{
  "name": "hazo_pdf",
  "version": "1.6.0",
  "description": "PDF viewer and annotater component for React",
  "keywords": [
    "pdf",
    "pdf-viewer",
    "react",
    "typescript",
    "annotation"
  ],
  "homepage": "https://github.com/pub12/hazo_pdf#readme",
  "bugs": {
    "url": "https://github.com/pub12/hazo_pdf/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pub12/hazo_pdf.git"
  },
  "license": "MIT",
  "author": "Pubs Abayasiri",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./styles.css": "./dist/styles/index.css",
    "./styles-full.css": "./dist/styles/full.css"
  },
  "files": [
    "dist",
    "config/hazo_pdf_config.ini",
    "LICENSE",
    "README.md"
  ],
  "peerDependencies": {
    "hazo_files": "^1.2.0",
    "hazo_llm_api": "^1.2.0",
    "hazo_logs": "^1.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "hazo_logs": {
      "optional": true
    },
    "hazo_files": {
      "optional": true
    },
    "hazo_llm_api": {
      "optional": true
    }
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "hazo_config": "^1.3.0",
    "hazo_connect": "^2.4.2",
    "lucide-react": "^0.553.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.10.38",
    "react-icons": "^5.5.0",
    "shadcn": "^3.5.0",
    "tailwind-merge": "^2.2.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "hazo_files": "^1.3.0",
    "hazo_llm_api": "^1.2.3",
    "hazo_logs": "^1.0.0",
    "next": "^16.0.3",
    "patch-package": "^8.0.1",
    "postcss": "^8.4.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^7.6.17",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-app:dev": "npm run build && concurrently -n tsup,next -c blue,green \"npm:dev\" \"next dev\"",
    "test-app:build": "next build",
    "test-app:start": "next start"
  }
}
